{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-sbx-ab092898-2"
		},
		"AzSqlDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().lsServerName}"
		},
		"AzSqlDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().lsDbName}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-sbx-ab092898-2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzSqlDb_properties_typeProperties_server": {
						"value": "[parameters('AzSqlDb_properties_typeProperties_server')]"
					},
					"AzSqlDb_properties_typeProperties_database": {
						"value": "[parameters('AzSqlDb_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}